<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="323.0" prefWidth="443.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uptc.ControllerView.TutorController">
   <children>
      <Label layoutX="30.0" layoutY="30.0" text="Nombre de la tutoría a editar:" />
      <TextField fx:id="nombreBuscarField" layoutX="200.0" layoutY="30.0" />

      <Label layoutX="30.0" layoutY="80.0" text="Nombre:" />
      <TextField fx:id="nombreFieldEdit" layoutX="200.0" layoutY="80.0" />

      <Label layoutX="30.0" layoutY="130.0" text="Descripción:" />
      <TextField fx:id="descripcionFieldEdit" layoutX="200.0" layoutY="130.0" />

      <Label layoutX="30.0" layoutY="180.0" text="Hora Inicio:" />
      <TextField fx:id="horaInicioFieldEdit" layoutX="200.0" layoutY="180.0" />

      <Label layoutX="30.0" layoutY="230.0" text="Hora Final:" />
      <TextField fx:id="horaFinalFieldEdit" layoutX="200.0" layoutY="230.0" />

      <Button layoutX="30.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleBuscarTutoria" text="Buscar Tutoría" />
      <Button layoutX="200.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleGuardarTutoriaEditada" text="Guardar Cambios" />
      <Button layoutX="370.0" layoutY="280.0" mnemonicParsing="false" onAction="#switchToMain" text="Volver" />
   </children>
</AnchorPane>
