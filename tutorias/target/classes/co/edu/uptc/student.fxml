<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uptc.ControllerView.StudentControl">
    <children>
        <Label text="Ingrese su id" />
        <TextField fx:id="idStudent" layoutX = "150.0" layoutY="50.0" />
        <HBox spacing="10.0" alignment="CENTER">
            <children>
                <Button fx:id="Back" text="Anterior" onAction="#switchToBack"/>
                <Button fx:id="next" text="Siguiente" onAction="#switchToValidId"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
