<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
    fx:controller="co.edu.uptc.ControllerView.TutorController">
    <children>
        <Label text="Ingrese el nombre del evento" />
        <TextField fx:id="nameEvent" layoutX="15" layoutY="50"  />
        <Label text="Ingrese la descripcion del evento" />
        <Label text="Ingrese el dia del evento (Lunes a Viernes)" />
        <Label text="Ingrese la hora inicial del evento (HH:MM)" />
        <Label text="Ingrese la hora final del evento (HH:MM)" />
        <Button fx:id="GuardarEvento"  text="Guardar"/ >
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>



