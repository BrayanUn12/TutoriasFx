<?xml version="1.0" encoding="UTF-8"?>

  

<?import javafx.scene.control.Button?>

<?import javafx.scene.control.CheckBox?>

<?import javafx.scene.control.Label?>

<?import javafx.scene.control.TextField?>

<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>

<?import javafx.scene.layout.AnchorPane?>



<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uptc.ControllerView.TutorController">
  <children>
    <TextField fx:id="nombreField" layoutX="50.0" layoutY="50.0" promptText="Nombre"/>
    <TextField fx:id="descripcionField" layoutX="50.0" layoutY="100.0" promptText="Descripción"/>
    <TextField fx:id="diaField" layoutX="50.0" layoutY="150.0" promptText="Día"/>
    <TextField fx:id="horaInicioField" layoutX="50.0" layoutY="200.0" promptText="Hora de Inicio"/>
    <TextField fx:id="horaFinalField" layoutX="50.0" layoutY="250.0" promptText="Hora de Finalización"/>
    <CheckBox fx:id="inscritoCheckBox" layoutX="50.0" layoutY="300.0" text="Inscrito"/>
    <Button fx:id="GuardarEvento" layoutX="50.0" layoutY="350.0" text="Guardar Evento" onAction="#handleGuardarTutoria"/>
    <Button fx:id="VolverMenu" layoutX="150.0" layoutY="350.0" text="Volver" onAction="#volverMenu"/>
  </children>
</AnchorPane>