<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<VBox alignment="TOP_LEFT" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uptc.ControllerView.StudentControl">
    <children>

        <AnchorPane layoutX="1.0" layoutY="1.0" prefHeight="45.0" prefWidth="307.0" style="-fx-background-color: #FFCC00; -fx-border-color: #D6DBDF;">
            <children>
                <Label fx:id="labelName" alignment="CENTER" layoutX="2.0" layoutY="12.0" prefHeight="21.0" prefWidth="307.0" text="BIENVENIDO">
                    <font>
                        <Font name="System Bold Italic" size="18.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>

        <Button layoutY="50.0" layoutX="20.0" fx:id="addTutoring" text="Agregar Tutoria" onAction="#switchToAddTutoring"/>
        <Button layoutY="80.0" layoutX="20.0" fx:id="deleteteTutoring" text="Eliminar Tutoria" onAction="#switchToDeleteTutoring"/>
        <Button layoutY="110.0" layoutX="20.0" fx:id="findTutoring" text="Buscar Tutorias de docente" onAction="#switchToFindTutoring"/>
        <Button layoutY="140.0" layoutX="20.0" fx:id="showTutoring" text="Mostrar tutorias agendadas" onAction="#switchToShowTutoring"/>

    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
